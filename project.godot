config_version=5

[application]

config/name="OutOfOrder"
run/main_scene="res://Scenes/ControllerIntro.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Sprites/misc/COOLCATS_presentsalt.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Sprites/characterProfiles/PotatoShredz.png"
config/windows_native_icon="res://Sprites/ico/icon.ico"
boot_splash/minimum_display_time=1500

[autoload]

Settings="*res://Data/Settings.gd"
Utils="*res://System/Utils.gd"
ComboManager="*res://Prefabs/combo_manager.gd"
