[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/size/mode=4
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/CustomEvents/plugin.cfg", "res://addons/StateMachine/plugin.cfg")

